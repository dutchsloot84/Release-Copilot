OPENAI_API_KEY=
OPENAI_PLANNER_MODEL=gpt-4o-mini
OPENAI_WORKER_MODEL=gpt-4o-mini
OPENAI_WRITER_MODEL=gpt-4o
MAX_TOKENS_PLANNER=1500
MAX_TOKENS_WORKER=2000
MAX_TOKENS_WRITER=4000

# Atlassian OAuth 2.0 (3LO)
ATLASSIAN_OAUTH_CLIENT_ID=your_client_id_here
ATLASSIAN_OAUTH_CLIENT_SECRET=your_client_secret_here

# Path to local OAuth token JSON (not in git)
JIRA_TOKEN_FILE=secrets/jira_oauth.json

# Jira Cloud/Server base URL (no trailing slash)
JIRA_BASE_URL=https://your-jira.example.com

BITBUCKET_BASE_URL=
BITBUCKET_EMAIL=
BITBUCKET_APP_PASSWORD=
BITBUCKET_PROJECT=

CONFLUENCE_ENABLED=false
CONFLUENCE_BASE_URL=
CONFLUENCE_EMAIL=
CONFLUENCE_API_TOKEN=
CONFLUENCE_SPACE_KEY=
CONFLUENCE_PARENT_PAGE_ID=

ENABLE_LLAMAINDEX=false

# Default JQL template; {fix_version} will be substituted
DEFAULT_JQL=fixVersion = "{fix_version}" AND issuetype not in ("Sub-task","Tech Story","Epic","Test Execution","Dev Task","QA Task","Shoulder Check","Automation","Test Plan","Spike","Test")
